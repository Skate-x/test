#include<stdio.h>

int main()
{
	int ch = 0;
	while((ch = getchar()) != EOF)
	{
		/*if(ch >= 'a' && ch <= 'z')
			return ch + 32;
		else if(ch >= 'A' && ch <= 'Z')
			return ch + 32;*/
		printf("%c\n", ch);
		putchar(ch);
	}
	return 0;
}

