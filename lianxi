#include<stdio.h>

int main()
{
	int a = 1;
	while(1<=a && a<=100)
	{
		if(a%2 == 1)
			printf("%d为奇数\n", a);
		else
			printf("%d为偶数\n", a);
		a++;
	};
	getchar();

}
