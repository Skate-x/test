#include<stdio.h>

int main()
{
	int i = 0;
	int  k= 0;
	for( i=0, k=0; k=0; i++, k++)//这里循环0次，因为k=0 赋值为假，无法进行循环
	{
		k++;
	}
	return 0;
}


